{
  "editor.tokenColorCustomizations": {
    "[Default Light+]": {
      "comments": "#F59BD4",
      "functions": "#07b897",
      "keywords": "#892eff",
      "numbers": "#DD028D",
      "strings": "#027f83",
      "types": "#44b0ee",
      "variables": "#DD028D",
      "textMateRules": [
        {
          "name": "main code",
          "scope": [
            "source.python",
            "souce.cpp",
            "source.r",
            "source.java",
            "source.txt",
            "source.md",
            "source.gcode",
            "source.html",
            "markup.italic.textit.latex",
            "meta.function.textit.latex",
            "text.tex.latex"
          ],
          "settings": {
            "foreground": "#DD028D"
          }
        },
        {
          "scope": [
            "support.function.builtin.python",
            "meta.function-call.generic.python"
          ],
          "settings": {
            "foreground": "#44b0ee"
          }
        },
        {
          "scope": [
            "variable.language.special.self.python",
            "support.variable.magic.python"
          ],
          "settings": {
            "foreground": "#DD028D"
          }
        },
        {
          "scope": [
            "keyword.operator.logical.python",
            "keyword.operator.comparison.python",
            "keyword.operator.arithmetic.python",
            "keyword.operator.assignment.python",
            "keyword.operator.python",
            "constant.language.python"
          ],
          "settings": {
            "foreground": "#892eff"
          }
        }
      ]
    }
  }
}
